{
  "parser": "@babel/eslint-parser",
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/prop-types": "off"
  },
  "globals": {
    "report": "readonly"
},
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
